# This file was generated by Qt Creator and will be overwritten unless you remove this line.
Hover:
  ShowAKA: Yes
Diagnostics:
  UnusedIncludes: Strict
Index:
  Background: Build
---
If:
  PathMatch: [.*\.hpp, .*\.cpp]
CompileFlags:
  Add:
    - -std=c++20
---
If:
  PathMatch: [.*\.h, .*\.c]
CompileFlags:
  Add:
    - -std=c17